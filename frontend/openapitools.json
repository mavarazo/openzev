{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "6.6.0",
    "generators": {
      "v1": {
        "generatorName": "typescript-angular",
        "output": "#{cwd}/src/generated-source/api",
        "glob": "./../backend/src/main/resources/api/main.yaml",
        "additionalProperties": {
          "ngVersion": "15.2.9",
          "npmName": "restClient",
          "supportsES6": "true",
          "npmVersion": "9.7.1",
          "withInterfaces": true,
          "stringEnums": true
        }
      }
    }
  }
}
